{
  "name": "Meme",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The first extension that I made.",
  "content_scripts": [{
    "matches": ["http://*/*"],
    "js": ["content_scripts.js", "link-back.js"]
  }],
  "icons": {
      "16": "icon.png",
      "48": "48.jpg",
      "128": "icon.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*"
  ]
}
